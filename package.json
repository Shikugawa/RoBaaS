{
  "name": "RoBaaS",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clear": "rm -rf ./public",
    "build": "parcel build app.js --no-cache -d public --target=node",
    "watch": "nodemon --watch config --exec \"npm run build && node public/app.js\"",
    "start-dev": "npm run clear && npm run watch"
  },
  "dependencies": {
    "dockerode": "^2.5.5",
    "express": "~4.16.0",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.3",
    "parcel-bundler": "^1.9.7"
  }
}
